{
    "name": "nova-voicebot-api",
    "version": "2.4.1",
    "description": "API for Nova VoiceBot",
    "license": "MIT",
    "private": true,
    "main": "dist/src/main.js",
    "scripts": {
        "clean": "rm -r dist/ || true",
        "compile": "npx -p typescript tsc && npx tsc-alias",
        "build": "yarn clean && yarn compile",
        "start": "yarn build && node .",
        "dev": "yarn build && nodemon .",
        "test": "yarn test:env && jest",
        "test:env": "cross-env NODE_ENV=test",
        "test:cov": "yarn test --coverage",
        "test:watch": "yarn test --watch",
        "lint": "eslint . --ext .ts,.json",
        "lint:fix": "eslint . --ext .ts,.json --fix",
        "format": "prettier --write ."
    },
    "engines": {
        "node": ">=16.16.0",
        "npm": ">=8.17.0"
    },
    "dependencies": {
        "@jest/globals": "^29.5.0",
        "@types/bcryptjs": "^2.4.2",
        "@types/cors": "^2.8.13",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.15",
        "@types/jest": "^29.5.0",
        "@types/jsonwebtoken": "^9.0.1",
        "@types/morgan": "^1.9.4",
        "@types/node": "^18.11.18",
        "@types/nodemailer": "^6.4.7",
        "@types/supertest": "^2.0.12",
        "axios": "^1.2.3",
        "bcryptjs": "^2.4.3",
        "cors": "^2.8.5",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "helmet": "^6.0.1",
        "jsonwebtoken": "^9.0.0",
        "mongoose": "^7.0.0",
        "morgan": "^1.10.0",
        "node-cache": "^5.1.2",
        "nodemailer": "^6.9.1",
        "openai": "^3.1.0",
        "tsc": "^2.0.4"
    },
    "devDependencies": {
        "@jest/globals": "^29.5.0",
        "@types/bcryptjs": "^2.4.2",
        "@types/cors": "^2.8.13",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.15",
        "@types/jest": "^29.5.0",
        "@types/jsonwebtoken": "^9.0.1",
        "@types/morgan": "^1.9.4",
        "@types/node": "^18.11.18",
        "@types/nodemailer": "^6.4.7",
        "@types/supertest": "^2.0.12",
        "@typescript-eslint/eslint-plugin": "^5.57.0",
        "@typescript-eslint/parser": "^5.57.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.37.0",
        "jest": "^29.5.0",
        "nodemon": "^2.0.20",
        "prettier": "^2.8.7",
        "supertest": "^6.3.3",
        "ts-jest": "^29.0.5",
        "ts-node": "^10.9.1",
        "tsc-alias": "^1.8.5",
        "typescript": "^5.0.2"
    },
    "nodemonConfig": {
        "ext": "ts,js,json"
    },
    "author": "Yacine BOUKARI <steeveboukari9@gmail.com>",
    "repository": {
        "type": "git",
        "url": "https://github.com/YacineSteeve/nova-voicebot-api.git"
    },
    "bugs": {
        "url": "https://github.com/YacineSteeve/nova-voicebot-api/issues"
    },
    "keywords": [
        "nova",
        "voicebot",
        "api",
        "nodejs",
        "typescript",
        "express",
        "mongodb",
        "openai",
        "gpt3",
