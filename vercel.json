{
    "version": 2,
    "name": "nova-voicebot-api",
    "builds": [
        {
            "src": "dist/main.js",
            "use": "@vercel/node",
            "config": { "includeFiles": ["dist/**"] }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/main.js"
        }
    ]
}
